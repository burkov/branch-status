{
  "name": "branch-status",
  "version": "1.4.2",
  "description": "CLI tool for monitoring branches state",
  "main": "build/index.js",
  "scripts": {
    "up": "npm-check-updates -u && typesync && yarn",
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development",
    "start": "yarn build:dev && node build/index.js",
    "link:local": "npm rm --global branch-status && npm link"
  },
  "bin": {
    "branch-status": "build/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/burkov/branch-status.git"
  },
  "keywords": [
    "cli",
    "youtrack",
    "git",
    "branch"
  ],
  "author": "abu",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/burkov/branch-status/issues"
  },
  "homepage": "https://github.com/burkov/branch-status#readme",
  "dependencies": {
    "axios": "^0.27.2",
    "chalk": "^5.0.1",
    "commander": "^9.3.0",
    "dayjs": "^1.11.2",
    "ora": "^6.1.0",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "@types/node": "^17.0.39",
    "@types/prettier": "^2.6.3",
    "@types/shelljs": "^0.8.11",
    "@webpack-cli/generators": "^2.4.2",
    "npm-check-updates": "^13.1.1",
    "prettier": "^2.6.2",
    "ts-loader": "^9.3.0",
    "typescript": "^4.7.3",
    "typesync": "^0.9.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2"
  }
}
